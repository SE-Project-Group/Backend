<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="model.BestFeed" table="bestfeed">
		<composite-id >  
            <key-property name="feedId" type="java.lang.String" column="feedId"></key-property>  
            <key-property name="date" type="java.sql.Date" column="date"></key-property>  
     	</composite-id>

	</class>
</hibernate-mapping>
